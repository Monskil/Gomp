package ordermanager

import(
	"queue"
	"global"
	"fmt"
	)

func HandleNewGlobal(globalOrderList chan [global.NUM_GLOBAL_ORDERS]queue.Order, internalOrderList chan [global.NUM_INTERNAL_ORDERS]queue.Order){
	for{
		select{
		case  <- globalOrderList:
			fmt.Println("I stole it mwuahahaha")
			//fmt.Println(updated, "handleupdatet did stuuff")
			//globalOrderList <- updated // fsm fyller på global. Det må skje noe her og fylle på channel igjen. fiks senere

			/* if you are the master:
			run cost-func, and decide who's getting the new order
			 if you are a slave:
			 send to the master/*
			 */
		case <- internalOrderList:
			fmt.Println("I stole it mwuahahaha")
			//fmt.Println(updatedInternal, "handleupdatet did stuff")
			//internalOrderList <- updatedInternal // fsm fyller på global. Det må skje noe her og fylle på channel igjen. fiks senere
		}
	}
}
